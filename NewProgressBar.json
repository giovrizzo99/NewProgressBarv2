{
    "id": "com.gr.sap.newprogressbar",
    "version": "2.0.1",
    "name": "New Progress Bar",
    "description": "New Progress Bar",
    "newInstancePrefix": "NewProgressBar",
    "supportsMobile": true,
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-gr-newprogressbar",
            "url": "https:\/\/cdn.jsdelivr.net\/gh\/giovrizzo99\/NewProgressBar@mainv201\/NewProgressBar.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-gr-newprogressbar-builder",
            "url": "https:\/\/cdn.jsdelivr.net\/gh\/giovrizzo99\/NewProgressBar@mainv201\/NewProgressBar_Builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "percentage": { "description": "Percentage", "type": "integer", "default": 0 },
        "barColor": { "description": "Bar Color", "type": "string", "default": "#00243b" },
        "emptyBarColor": { "description": "Empty Bar Color", "type": "string", "default": "#f0f0f0" },
        "startColor": { "description": "Start Gradient Color", "type": "string", "default": "#05446b" },
        "endColor": { "description": "End Gradient Color", "type": "string", "default": "#69a8e2" }
    },
    "methods": {
        "setPercentage": {
            "description": "Set Percentage",
            "parameters": [
                {
                    "name": "percentage",
                    "type": "integer",
                    "description": "Percentage"
                }
            ],
            "body": "this.percentage = percentage;"
        },
        "getPercentage": {
            "returnType": "integer",
            "description": "Return Percentage",
            "body": "return this.percentage;"
        },
        "setBarColor": {
            "description": "Set Bar Color",
            "parameters": [
                {
                    "name": "barColor",
                    "type": "string",
                    "description": "Bar Color"
                }
            ],
            "body": "this.barColor = barColor;"
        },
        "getBarColor": {
            "returnType": "string",
            "description": "Return Bar Color",
            "body": "return this.barColor;"
        },
        "setEmptyBarColor": {
            "description": "Set Empty Bar Color",
            "parameters": [
                {
                    "name": "emptyBarColor",
                    "type": "string",
                    "description": "Empty Bar Color"
                }
            ],
            "body": "this.emptyBarColor = emptyBarColor;"
        },
        "getEmptyBarColor": {
            "returnType": "string",
            "description": "Return Empty Bar Color",
            "body": "return this.emptyBarColor;"
        }
    },
    "events": {
        "onClick": {
            "description": "User Clicked."
        }
    }
}
